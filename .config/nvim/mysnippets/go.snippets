# Snippets for Go

priority -9

# test table for buffalo snippet
snippet btt
var tests = []struct {
	name string
	expected string
	given string
}{
	{"${1}", "${2}", "${3}",},
}
for _, tt := range tests {
	tt := tt

	actual := ${0:${VISUAL}}(tt.given)
	as.Equal(tt.expected, actual, tt.name)
}
endsnippet

