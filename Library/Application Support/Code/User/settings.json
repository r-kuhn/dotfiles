{
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.showBracketsInRuler": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.cursorBlinking": "smooth",
    // Operator Mono Font Settings:
    "editor.fontFamily": "OperatorMonoLig Nerd Font",
    "editor.fontSize": 14,
    "terminal.integrated.fontFamily": "FuraCode Nerd Font",
    "terminal.integrated.fontSize": 13,
    // Input Mono font settings:
    // "editor.fontFamily": "Input Mono Condensed",
    // "terminal.integrated.fontFamily": "Input Mono Condensed",
    // "editor.fontSize": 13,
    // "terminal.integrated.fontSize": 13,
    // Iosevka font settings:
    // "terminal.integrated.fontFamily": "Iosevka Term",
    // "editor.fontFamily": "Iosevka",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "gitlens.advanced.telemetry.enabled": false,
    "go.alternateTools": {
      "go-langserver": "gopls"
    },
    "go.buildOnSave": "off",
    "go.vetOnSave": "off",
    "go.useLanguageServer": true,
    "go.formatTool": "goimports",
    "go.autocompleteUnimportedPackages": true,
    "go.languageServerExperimentalFeatures": {
      "format": true,
      "autoComplete": true,
      "diagnostics": true,
      "goToDefinition": true,
      "hover": true,
      "signatureHelp": true,
      "goToTypeDefinition": true,
      "documentSymbols": true
    },
    // "go.lintFlags": [ "--fast" ],
    // "go.lintTool": "golangci-lint",
    "[go]": {
      "editor.insertSpaces": false,
      "editor.formatOnSave": true,
      "editor.codeActionsOnSave": {
        "source.organizeImports": true
      }
    },
    "gopls": {
      "enablePlaceholders": true,
      "enhancedHover": true
    },
    "gopls.env": {
      "GOFLAGS": "--tags=development"
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terraform.telemetry.enabled": false,
    "vim.easymotion": false,
    "vim.handleKeys": {
      "<C-a>": false,
      "<C-f>": false,
      "<C-e>": false
    },
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.insertModeKeyBindings": [
      {
        "after": ["<Esc>"],
        "before": ["j", "j"]
      }
    ],
    "vim.leader": "<space>",
    "vim.visualModeKeyBindingsNonRecursive": [
      {
        "before": [">"],
        "commands": ["editor.action.indentLines"]
      },
      {
        "before": ["<"],
        "commands": ["editor.action.outdentLines"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "after": ["d", "d"],
        "before": ["<leader>", "d"]
      },
      {
        "before": ["<C-n>"],
        "commands": [":nohl"]
      }
    ],
    "vim.sneak": false,
    "vim.useCtrlKeys": true,
    "vim.useSystemClipboard": true,
    "vim.highlightedyank.enable": true,
    "workbench.fontAliasing": "antialiased",
    "workbench.statusBar.feedback.visible": false,
    "window.zoomLevel": 0,
    "emmet.includeLanguages": {
      "erb": "erb",
      "html": "html"
    },
    "workbench.iconTheme": "Monokai Pro (Filter Ristretto) Icons",
    "extensions.ignoreRecommendations": false,
    "breadcrumbs.enabled": false,
    "workbench.colorTheme": "Monokai Pro (Filter Ristretto)",
    "workbench.startupEditor": "newUntitledFile",
    "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "mjml.autoPreview": true,
    "mjml.beautifyHtmlOutput": true,
    "mjml.templateGallery": true,
    "errorLens.warningColor": "rgba(240,0,0,0.4)",
    "editor.hover.enabled": true,
    "explorer.autoReveal": false,
    "aws.samcli.location": "/Users/dan/brew/bin/sam",
    "aws.profile": "default",
    "workbench.activityBar.visible": true,
    "mjml.minifyHtmlOutput": false
  }