{
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "revealInExplorer"
      ],
    },
    {
      "before": [
        "<leader>",
        "r",
        "f"
      ],
      "commands": [
        "fileutils.renameFile"
      ]
    }
  ],
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "go.lintTool": "golangci-lint",
  "go.autocompleteUnimportedPackages": true,
  "go.languageServerExperimentalFeatures": {
    "format": true,
    "diagnostics": true,
    "documentLink": true
  },
  "workbench.iconTheme": "Monokai Pro (Filter Ristretto) Icons",
  "workbench.colorTheme": "Monokai Pro (Filter Ristretto)",
  "rust-analyzer.highlightingOn": true,
  "rust-analyzer.enableEnhancedTyping": false,
  "rust-analyzer.featureFlags": {
    "notifications.cargo-toml-not-found": false
  },
  "rust-analyzer.cargo-watch.enable": true,
  "rust-analyzer.cargo-watch.command": "clippy",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.useColorContributions": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.fontFamily": "OperatorMonoLig Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "editor.hover.delay": 3000,
  "editor.hover.enabled": false,
  "editor.parameterHints.enabled": false,
  "[html]": {
    "editor.formatOnSave": false
  },
  "emmet.includeLanguages": {
    "erb": "erb",
    "html": "html"
  },
  "files.associations": {
    "*.plush.html": "erb"
  },
  "extensions.ignoreRecommendations": false,
}