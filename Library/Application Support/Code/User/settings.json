{
  "vim.easymotion": true,
  "vim.easymotionMarkerYOffset": 0,
  "vim.easymotionMarkerFontFamily": "Menlo",
  "vim.easymotionMarkerFontSize": "12",
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": false,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "revealInExplorer"
      ],
    },
    {
      "before": [
        "<leader>",
        "r",
        "f"
      ],
      "commands": [
        "fileutils.renameFile"
      ]
    }
  ],
  "vim.leader": ",",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-e>": false,
    "<C-g>": false
  },
  "go.formatTool": "goimports",
  "go.lintTool": "golangci-lint",
  "go.autocompleteUnimportedPackages": true,
  "go.useLanguageServer": true,
  "go.languageServerExperimentalFeatures": {
    "format": false,
    "rename": true,
    "diagnostics": true,
    "documentLink": true,
    "findReferences": true,
    "goToDefinition": true,
    "goToTypeDefinition": true,
    "hover": true,
    "signatureHelp": true,
    "workspaceSymbols": true
  },
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    }
  },
  "workbench.iconTheme": "Monokai Pro Icons",
  "rust-analyzer.highlightingOn": true,
  "rust-analyzer.enableEnhancedTyping": true,
  "rust-analyzer.featureFlags": {
    "notifications.cargo-toml-not-found": false
  },
  "rust-analyzer.cargo-watch.enable": true,
  "rust-analyzer.cargo-watch.command": "clippy",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.useColorContributions": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.fontFamily": "OperatorMonoLig Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "terminal.integrated.fontFamily": "OperatorMonoLig Nerd Font",
  "editor.hover.delay": 3000,
  "editor.hover.enabled": true,
  "editor.parameterHints.enabled": false,
  "[html]": {
    "editor.formatOnSave": false
  },
  "emmet.includeLanguages": {
    "erb": "erb",
    "html": "html"
  },
  "files.associations": {
    "*.plush.html": "erb"
  },
  "extensions.ignoreRecommendations": false,
  "workbench.activityBar.visible": false,
  "breadcrumbs.enabled": false,
  "window.zoomLevel": 0,
  "terminal.integrated.fontSize": 14,
  "files.insertFinalNewline": true,
  "sqltools.connections": [
    {
      "connectString": "postgres://postgres:postgres@localhost:5432/reflector_development",
      "dialect": "PostgreSQL",
      "name": "reflector_development"
    }
  ],
  "workbench.colorTheme": "Monokai Pro",
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.advanced.telemetry.enabled": false,
  "sqltools.telemetry": false,
}
